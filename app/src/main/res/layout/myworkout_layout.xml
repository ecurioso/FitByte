        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/workoutName"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:textColor="#ff090909"
